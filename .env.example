# Google Gemini API Key
# Get your API key from: https://ai.google.dev/
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
# Get these from your Supabase project settings: https://supabase.com/dashboard
VITE_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
VITE_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# RevenueCat Public SDK Key
# Create in RevenueCat dashboard (Project Settings -> API Keys)
VITE_REVENUECAT_API_KEY=your_revenuecat_public_sdk_key_here

# Optional RevenueCat targeting overrides
# If set, placement is checked first, then offering ID, then current default offering.
VITE_REVENUECAT_PLACEMENT_ID=
VITE_REVENUECAT_OFFERING_ID=
